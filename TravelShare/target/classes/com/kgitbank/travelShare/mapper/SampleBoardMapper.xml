<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.kgitbank.travelShare.mapper.SampleBoardMapper">
	<select id="getBoardsampleAll" resultType="com.kgitbank.travelShare.model.SampleBoardModel">
		SELECT * FROM boardsample
	</select>
	
	<select id="getBoardsamplePaging" resultType="com.kgitbank.travelShare.model.SampleBoardModel">
		SELECT board_id, user_name, board_name, area 
		FROM (SELECT ROWNUM AS RNUM, board_id.*
        FROM boardsample board_id) WHERE RNUM BETWEEN #{startnum } AND #{endnum }-1
	</select>
</mapper>